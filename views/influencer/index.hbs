<div class="stepper">
  <a href="/users">Home</a> /
  <a href="/users/{{userId}}">Profile</a> / Influencers
</div>

<div class="store-header">
  <h1>These are {{user.firstName}}'s Influencers</h1>
  <div class="button primary">
    <a href="/users/{{user._id}}/influencer/new">Add a Influencer</a>
  </div>
</div>



<div class="stores">
  {{#each user.influencers}}
      <div>Name:{{this.name}}</div>
      <div>Instagram:{{this.instagram}}</div> 
      <div>City:{{this.location}}</div>
      <div><a href="/users/{{user._id}}/influencer/{{this._id}}/style">Style</a></div>
      <div><img src="{{this.photoUrl}}" alt="broken"></div>
      <hr>
  {{/each}}






 
 <div>
   {{#each user.influencer}}
        {{#each this.style}}
          {{this.img1}}
          {{this.img2}}
        {{/each}}
     <div>
       <a href="/users/{{../userId}}/influencer/{{../influencerId}}/style/{{this._id}}">
         banana
       </a>
     </div>
   {{/each}}
 </div>

  
</div>